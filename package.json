  "dependencies": {
    "awe-dnd": "^0.3.4",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.0",
    "core-js": "^2.6.5",
    "iview": "^3.4.1",
    "json-bigint": "^0.3.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "qrcode": "^1.3.3",
    "routes": "^2.1.0",
    "vue": "^2.6.6",
    "vue-clipboard2": "^0.3.0",
    "vue-cookies": "^1.5.13",
    "vue-router": "^3.0.3",
    "vue-ueditor-wrap": "^2.4.1",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
